<form method="post" action="{{ path('liste_delete', {'id': liste.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ liste.id) }}">
    <button class="btn" onclick="return confirm('Etes-vous sur de vouloir supprimer cet article !')"> <i  style="cursor: pointer;" class="material-icons">delete</i></button>

</form>
